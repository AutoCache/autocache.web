<h1>Listing radios</h1>

<table>
  <tr>
    <th>Preset</th>
    <th>Style</th>
    <th>City</th>
    <th>State</th>
    <th>Frequency</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @radios.each do |radio| %>
  <tr>
    <td><%= radio.preset %></td>
    <td><%= radio.style %></td>
    <td><%= radio.city %></td>
    <td><%= radio.state %></td>
    <td><%= radio.frequency %></td>
    <td><%= radio.user_id %></td>
    <td><%= link_to 'Show', radio %></td>
    <td><%= link_to 'Edit', edit_radio_path(radio) %></td>
    <td><%= link_to 'Destroy', radio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Radio', new_radio_path %>
